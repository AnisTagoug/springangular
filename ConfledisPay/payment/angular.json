{
  "version": 1,
  "projects": {
    "confledis": {
      "projectType": "application",
      "root": "apps/confledis",
      "sourceRoot": "apps/confledis/src",
      "prefix": "payment",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/confledis",
            "index": "apps/confledis/src/index.html",
            "main": "apps/confledis/src/main.ts",
            "polyfills": "apps/confledis/src/polyfills.ts",
            "tsConfig": "apps/confledis/tsconfig.app.json",
            "assets": [
              "apps/confledis/src/favicon.ico",
              "apps/confledis/src/assets"
            ],
            "styles": [
              "apps/confledis/src/styles.css"
            ],
            "scripts": [],
            "extraWebpackConfig": "apps/confledis/webpack.config.js",
            "commonChunk": false
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/confledis/src/environments/environment.ts",
                  "with": "apps/confledis/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "extraWebpackConfig": "apps/confledis/webpack.prod.config.js"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "confledis:build:production",
              "extraWebpackConfig": "apps/confledis/webpack.prod.config.js"
            },
            "development": {
              "browserTarget": "confledis:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "extraWebpackConfig": "apps/confledis/webpack.config.js",
            "port": 3000
          }
        },
        "extract-i18n": {
          "builder": "ngx-build-plus:extract-i18n",
          "options": {
            "browserTarget": "confledis:build",
            "extraWebpackConfig": "apps/confledis/webpack.config.js"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/confledis/src/**/*.ts",
              "apps/confledis/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "ngx-build-plus:karma",
          "outputs": [
            "coverage/apps/confledis"
          ],
          "options": {
            "jestConfig": "apps/confledis/jest.config.js",
            "passWithNoTests": true,
            "extraWebpackConfig": "apps/confledis/webpack.config.js"
          }
        }
      }
    },
    "confledis-e2e": {
      "root": "apps/confledis-e2e",
      "sourceRoot": "apps/confledis-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/confledis-e2e/cypress.json",
            "tsConfig": "apps/confledis-e2e/tsconfig.e2e.json",
            "devServerTarget": "confledis:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "confledis:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/confledis-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "lib-name": {
      "projectType": "library",
      "root": "libs/lib-name",
      "sourceRoot": "libs/lib-name/src",
      "prefix": "payment",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:ng-packagr-lite",
          "outputs": [
            "dist/libs/lib-name"
          ],
          "options": {
            "project": "libs/lib-name/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/lib-name/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/lib-name/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/lib-name"
          ],
          "options": {
            "jestConfig": "libs/lib-name/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/lib-name/src/**/*.ts",
              "libs/lib-name/src/**/*.html"
            ]
          }
        }
      }
    },
    "my-app": {
      "projectType": "application",
      "root": "apps/my-app",
      "sourceRoot": "apps/my-app/src",
      "prefix": "payment",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/my-app",
            "index": "apps/my-app/src/index.html",
            "main": "apps/my-app/src/main.ts",
            "polyfills": "apps/my-app/src/polyfills.ts",
            "tsConfig": "apps/my-app/tsconfig.app.json",
            "assets": [
              "apps/my-app/src/favicon.ico",
              "apps/my-app/src/assets"
            ],
            "styles": [
              "apps/my-app/src/styles.css"
            ],
            "scripts": [],
            "extraWebpackConfig": "apps/my-app/webpack.config.js",
            "commonChunk": false
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/my-app/src/environments/environment.ts",
                  "with": "apps/my-app/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "extraWebpackConfig": "apps/my-app/webpack.prod.config.js"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "my-app:build:production",
              "extraWebpackConfig": "apps/my-app/webpack.prod.config.js"
            },
            "development": {
              "browserTarget": "my-app:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "extraWebpackConfig": "apps/my-app/webpack.config.js",
            "port": 3000
          }
        },
        "extract-i18n": {
          "builder": "ngx-build-plus:extract-i18n",
          "options": {
            "browserTarget": "my-app:build",
            "extraWebpackConfig": "apps/my-app/webpack.config.js"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/my-app/src/**/*.ts",
              "apps/my-app/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "ngx-build-plus:karma",
          "outputs": [
            "coverage/apps/my-app"
          ],
          "options": {
            "jestConfig": "apps/my-app/jest.config.js",
            "passWithNoTests": true,
            "extraWebpackConfig": "apps/my-app/webpack.config.js"
          }
        }
      }
    },
    "my-app-e2e": {
      "root": "apps/my-app-e2e",
      "sourceRoot": "apps/my-app-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/my-app-e2e/cypress.json",
            "tsConfig": "apps/my-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "my-app:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "my-app:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/my-app-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "my-lib": {
      "projectType": "library",
      "root": "libs/my-lib",
      "sourceRoot": "libs/my-lib/src",
      "prefix": "payment",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/my-lib"
          ],
          "options": {
            "jestConfig": "libs/my-lib/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/my-lib/src/**/*.ts",
              "libs/my-lib/src/**/*.html"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false,
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  },
  "defaultProject": "confledis"
}